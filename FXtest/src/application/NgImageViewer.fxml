<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ngImageController">
   <children>
      <ImageView fx:id="ngImage" fitHeight="480.0" fitWidth="640.0" layoutX="30.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Button fx:id="nextBtn" layoutX="672.0" layoutY="155.0" mnemonicParsing="false" onAction="#onNext" prefHeight="242.0" prefWidth="27.0" text="&gt;">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <effect>
            <DropShadow color="#139eda" />
         </effect>
      </Button>
      <Button fx:id="prevBtn" layoutY="155.0" mnemonicParsing="false" onAction="#onPrev" prefHeight="242.0" prefWidth="27.0" text="&lt;">
         <font>
            <Font name="System Bold" size="13.0" />
         </font>
         <effect>
            <DropShadow color="#139eda" />
         </effect>
      </Button>
      <Button fx:id="closeBtn" layoutX="266.0" layoutY="549.0" mnemonicParsing="false" onAction="#onClose" prefHeight="26.0" prefWidth="75.0" text="CLOSE" />
      <Label fx:id="info1" layoutX="524.0" layoutY="503.0" prefHeight="26.0" prefWidth="148.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
